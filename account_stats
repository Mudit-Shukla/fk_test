            "acc.accountid, \n" +
            "case when openDate = '0000-00-00 00:00:00' or openDate = '' then -1 else DATEDIFF(NOW(), IFNULL(openDate, NOW())) end AS accountAgeInDays,\n" +
            "COUNT(DISTINCT ad.Pincode) AS distinct_pincode,\n" +
            "COUNT(transaction.TransactionId) AS transaction_count,\n" +
            "SUM(Amount) AS total_amount,\n" +
