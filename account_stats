            "COUNT(CASE WHEN OrderStatus = 3 AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN 1 END) AS cancelled_trxn_count_last_3ms,\n" +
            "\n" +
            "SUM(CASE WHEN OrderStatus = 4 or OrderStatus = 7 THEN Amount END) AS delivered_trxn_amount,\n" +
            "SUM(CASE WHEN OrderStatus = 5 or OrderStatus = 6 THEN Amount END) AS returned_trxn_amount,\n" +
            "SUM(CASE WHEN OrderStatus = 9 THEN Amount END) AS rejected_trxn_amount,\n" +
            "SUM(CASE WHEN OrderStatus = 3 THEN Amount END) AS cancelled_trxn_amount,\n" +
            "SUM(CASE WHEN (OrderStatus = 4 or OrderStatus = 7) AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 30 DAY) AND NOW() THEN Amount END) AS delivered_trxn_amount_1m,\n" +
            "SUM(CASE WHEN (OrderStatus = 5 or OrderStatus = 6) AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 30 DAY) AND NOW() THEN Amount END) AS returned_trxn_amount_1m,\n" +
            "SUM(CASE WHEN OrderStatus = 9 AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 30 DAY) AND NOW() THEN Amount END) AS rejected_trxn_amount_1m,\n" +
            "SUM(CASE WHEN OrderStatus = 3 AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 30 DAY) AND NOW() THEN Amount END) AS cancelled_trxn_amount_1m,\n" +
            "SUM(CASE WHEN (OrderStatus = 4 or OrderStatus = 7) AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN Amount END) AS delivered_trxn_amount_last_3ms,\n" +
            "SUM(CASE WHEN (OrderStatus = 5 or OrderStatus = 6) AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN AMount END) AS returned_trxn_amount_last_3ms,\n" +
            "SUM(CASE WHEN OrderStatus = 9 AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN Amount END) AS rejected_trxn_amount_last_3ms,\n" +
            "SUM(CASE WHEN OrderStatus = 3 AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN Amount END) AS cancelled_trxn_amount_last_3ms,\n" +
            "\n" +
            "COUNT(CASE WHEN TransactionModeId = 'CREDIT' THEN 1 END) AS credit_card_trxn_count,\n" +
            "COUNT(CASE WHEN TransactionModeId = 'DEBIT' THEN 1 END) AS debit_card_trxn_count,\n" +
            "COUNT(CASE WHEN TransactionModeId = 'PHONEPE' OR TransactionModeId = 'UPI_INTENT' OR TransactionModeId = 'UPI_COLLECT' OR TransactionModeId = 'FK_UPI' THEN 1 END) AS upi_trxn_count,\n" +
            "COUNT(CASE WHEN TransactionModeId = 'EGV' OR TransactionModeId = 'EGV_WALLET' OR TransactionModeId = 'WALLET' THEN 1 END) AS egv_wallets_trxn_count,\n" +
            "COUNT(CASE WHEN TransactionModeId = 'COD' THEN 1 END) AS cod_trxn_count,\n" +
            "COUNT(CASE WHEN TransactionModeId = 'EMI' THEN 1 END) AS emi_trxn_count,\n" +
            "COUNT(CASE WHEN TransactionModeId = 'FLIPKART_FINANCE' THEN 1 END) AS flipkart_finance_trxn_count,\n" +
            "COUNT(CASE WHEN (TransactionModeId = 'CREDIT') AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN 1 END) AS credit_card_trxn_count_last_3ms,\n" +
            "COUNT(CASE WHEN (TransactionModeId = 'DEBIT') AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN 1 END) AS debit_card_trxn_count_last_3ms,\n" +
            "COUNT(CASE WHEN (TransactionModeId = 'PHONEPE' OR TransactionModeId = 'UPI_INTENT' OR TransactionModeId = 'UPI_COLLECT' OR TransactionModeId = 'FK_UPI') AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN 1 END) AS upi_trxn_count_last_3ms,\n" +
            "COUNT(CASE WHEN (TransactionModeId = 'EGV' OR TransactionModeId = 'EGV_WALLET' OR TransactionModeId = 'WALLET') AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN 1 END) AS egv_wallets_trxn_count_last_3ms,\n" +
            "COUNT(CASE WHEN (TransactionModeId = 'COD') AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN 1 END) AS cod_trxn_count_last_3ms,\n" +
            "COUNT(CASE WHEN (TransactionModeId = 'EMI') AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN 1 END) AS emi_trxn_count_last_3ms,\n" +
            "COUNT(CASE WHEN (TransactionModeId = 'FLIPKART_FINANCE') AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN 1 END) AS flipkart_finance_trxn_count_last_3ms,\n" +
            "SUM(CASE WHEN TransactionModeId = 'CREDIT' THEN Amount END) AS credit_card_trxn_amount,\n" +
            "SUM(CASE WHEN TransactionModeId = 'DEBIT' THEN Amount END) AS debit_card_trxn_amount,\n" +
            "SUM(CASE WHEN TransactionModeId = 'PHONEPE' OR TransactionModeId = 'UPI_INTENT' OR TransactionModeId = 'UPI_COLLECT' OR TransactionModeId = 'FK_UPI' THEN Amount END) AS upi_trxn_amount,\n" +
            "SUM(CASE WHEN TransactionModeId = 'EGV' OR TransactionModeId = 'EGV_WALLET' OR TransactionModeId = 'WALLET' THEN Amount END) AS egv_wallets_trxn_amount,\n" +
            "SUM(CASE WHEN TransactionModeId = 'COD' THEN Amount END) AS cod_trxn_amount,\n" +
            "SUM(CASE WHEN TransactionModeId = 'EMI' THEN Amount END) AS emi_trxn_amount,\n" +
            "SUM(CASE WHEN TransactionModeId = 'FLIPKART_FINANCE' THEN Amount END) AS flipkart_finance_trxn_amount,\n" +
            "SUM(CASE WHEN (TransactionModeId = 'CREDIT') AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN Amount END) AS credit_card_trxn_amount_last_3ms,\n" +
            "SUM(CASE WHEN (TransactionModeId = 'DEBIT') AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN Amount END) AS debit_card_trxn_amount_last_3ms,\n" +
            "SUM(CASE WHEN (TransactionModeId = 'PHONEPE' OR TransactionModeId = 'UPI_INTENT' OR TransactionModeId = 'UPI_COLLECT' OR TransactionModeId = 'FK_UPI') AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN Amount END) AS upi_trxn_amount_last_3ms,\n" +
            "SUM(CASE WHEN (TransactionModeId = 'EGV' OR TransactionModeId = 'EGV_WALLET' OR TransactionModeId = 'WALLET') AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN Amount END) AS egv_wallets_trxn_amount_last_3ms,\n" +
            "SUM(CASE WHEN (TransactionModeId = 'COD') AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN Amount END) AS cod_trxn_amount_last_3ms,\n" +
            "SUM(CASE WHEN (TransactionModeId = 'EMI') AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN Amount END) AS emi_trxn_amount_last_3ms,\n" +
            "SUM(CASE WHEN (TransactionModeId = 'FLIPKART_FINANCE') AND TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN Amount END) AS flipkart_finance_trxn_amount_last_3ms,\n" +
            "\n" +
            "SUM(CASE WHEN (card.IsInternational = 1) and TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN 1 END) AS international_trxn_count_last_3ms, \n" +
            "SUM(CASE WHEN (card.IsInternational = 1) and TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 180 DAY) AND NOW() THEN 1 END) AS international_trxn_count_last_6ms, \n" +
            "SUM(CASE WHEN (card.IsInternational = 1) and TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 30 DAY) AND NOW() THEN 1 END) AS international_trxn_count_last_month, \n" +
            "SUM(CASE WHEN (card.IsInternational = 1) and TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 90 DAY) AND NOW() THEN Amount END) AS international_trxn_sum_last_3ms, \n" +
            "SUM(CASE WHEN (card.IsInternational = 1) and TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 180 DAY) AND NOW() THEN Amount END) AS international_trxn_sum_last_6ms, \n" +
            "SUM(CASE WHEN (card.IsInternational = 1) and TransactionDate BETWEEN DATE_SUB(NOW(), INTERVAL 30 DAY) AND NOW() THEN Amount END) AS international_trxn_sum_last_month, \n" +
            "\n" +
            "NOW()\n" +
            "FROM intellewings_backup.transaction as transaction\n" +
            "JOIN intellewings_backup.account AS acc ON transaction.AccountId = acc.AccountId\n" +
            "left JOIN intellewings_backup.card ON transaction.CardId = card.CardId\n" +
            "left JOIN intellewings.backup.address AS ad ON ad.TransactionId = transaction.TransactionId\n" +
            "WHERE transaction.ACCOUNTID IN (parameter)\n" +
            "GROUP BY accountid ;\n";

